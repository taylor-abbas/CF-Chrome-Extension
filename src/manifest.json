{
  "manifest_version": 2,
  "name": "CF Rating Finder",
  "version": "1.0",
  "description": "This is a simple extension that displays your current CF Rating",
  "author": "taylor abbas",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js; object-src 'self'",
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}